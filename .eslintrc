{
    "extends": "eslint",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
        "node/no-unpublished-require": "off",
        "no-underscore-dangle": "off",
        "new-cap" : "off",
        "jsdoc/newline-after-description": "off"
    }
}